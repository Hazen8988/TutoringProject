@model IEnumerable<TutoringProject.Models.Session.Session>

@{
    ViewBag.Title = "Session List";
}

<h2>Session List</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.First().Id)</th>
        <th>@Html.DisplayNameFor(model => model.First().Time)</th>
        <th>@Html.DisplayNameFor(model => model.First().Course.Name)</th>
        <th>@Html.DisplayNameFor(model => model.First().IsOnline)</th>
        <th>Tutor</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        @Html.Partial("_SessionIndexItem", item)
    }

</table>
